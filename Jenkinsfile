pipline {
    agent any
    stages {
      stage('Build'){
        steps {
          echo "Build steps"
          sleep 4  
          }
          
      
      }
         stage('Test'){
           steps {
             echo "Test steps"
             sleep 5  
          }
          
      
      }
         stage('Deploy'){
           steps {
             echo "Deploy war file"
             sleep 10  
          }
             
          
      }
        
    }

}
