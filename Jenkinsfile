pipeline {
    agent any 
    stages {
      stage('Build'){
        steps {
          echo "Build step"
          sleep 4
        }
        
      }
      stage('Test'){
        steps {
          echo "Test step"
          sleep 5
        }
        
      }
      stage('deploy'){
        steps {
          echo "deploy war file"
          sleep 10
        }
        
      }
      
        
    }


}
